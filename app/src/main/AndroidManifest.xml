<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iavariav.root.joon">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera.front" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.iavariav.root.joon.LoginActivity" />
        <activity
            android:name="com.iavariav.root.joon.Rule.HomeActivity"
            android:label="JOON"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="com.iavariav.root.joon.Rule.DetailNewsActivity"
            android:label="Detail Berita"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.iavariav.root.joon.Rule.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.iavariav.root.joon.Rule.ProfilActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.PartaiActivity"
            android:label="Partai"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.DapilActivity"
            android:label="Dapil"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.DetailDapilActivity"
            android:label="Detail Dapil"
            android:parentActivityName="com.iavariav.root.joon.Rule.DapilActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.iavariav.root.joon.Rule.DapilActivity" />
        </activity>
        <activity
            android:name="com.iavariav.root.joon.Rule.DetailPartaiActivity"
            android:label="Detail Partai"
            android:parentActivityName="com.iavariav.root.joon.Rule.PartaiActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.iavariav.root.joon.Rule.PartaiActivity" />
        </activity>
        <activity
            android:name="com.iavariav.root.joon.Rule.Anggaran.AnggaranActivity"
            android:label="Anggaran"
            android:parentActivityName="com.iavariav.root.joon.Rule.WIlayahActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.Anggaran.DetailAnggaranActivity"
            android:label="Detail Anggaran"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.TahapanPemiluActivity"
            android:label="Tahapan Pemilu"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.TataTertibActivity"
            android:label="Tata Tertib"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.WIlayahActivity"
            android:label="Wilayah"
            android:parentActivityName="com.iavariav.root.joon.Rule.Anggaran.DaftarAnggaranActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.Anggaran.AnggaranPribadiActivity"
            android:label="Anggaran Pribadi"
            android:parentActivityName="com.iavariav.root.joon.Rule.WIlayahActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.Anggaran.DaftarAnggaranActivity"
            android:label="Pilih Anggaran"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.Anggaran.DetailAnggaranPribadiActivity"
            android:label="Detail Anggaran Pribadi"
            android:parentActivityName="com.iavariav.root.joon.Rule.Anggaran.AnggaranPribadiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.Anggaran.AnggaranPartaiActivity"
            android:label="Anggaran Partai"
            android:parentActivityName="com.iavariav.root.joon.Rule.WIlayahActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.Anggaran.DetailAnggaranPartaiActivity"
            android:label="Detail Anggaran Partai"
            android:parentActivityName="com.iavariav.root.joon.Rule.Anggaran.AnggaranPartaiActivity"
            android:screenOrientation="portrait" />

        <service
            android:name="com.iavariav.root.joon.Service.MyService"
            android:process=".my_service" />

        <activity
            android:name="com.iavariav.root.joon.Rule.TataCaraActivity"
            android:label="Tata Cara"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.FAQActivity"
            android:label="FAQ"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.IklanVideoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.IklanListActivity"
            android:label="Iklan Masyarakat"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.iavariav.root.joon.Rule.LaporanAkhirPemilihActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.iavariav.root.joon.Rule.PantauKampanyeActivity"></activity>
    </application>

</manifest>